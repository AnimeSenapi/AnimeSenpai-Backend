{
  "version": 2,
  "buildCommand": "prisma generate",
  "installCommand": "npm install",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.2.29"
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/:path*",
      "destination": "/api"
    },
    {
      "source": "/health",
      "destination": "/api"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://animesenpai.app",
      "permanent": true
    }
  ]
}
